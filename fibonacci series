n1=0
n2=1
count=0
a= int( input("enter the no of trems") )
if a<=1:
    print("the fibonacci series is: ",n1)
elif a==2:
    print("the fibonacci series is:",n1,n2)
else:
    while count< a:
        print(n1)
        y=n1+n2
        n1=n2
        n2=y
        count += 1
        print("the fibonacci series is:")
        
            
